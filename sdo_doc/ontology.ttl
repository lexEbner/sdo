@prefix : <http://example.org/sdo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/sdo#> .

<http://example.org/sdo#> rdf:type owl:Ontology ;
                           <http://purl.org/dc/terms/created> "2025-09-25"^^xsd:date ;
                           <http://purl.org/dc/terms/creator> "http://example.org/people#AlexanderEbner" ;
                           <http://purl.org/dc/terms/description> "An ontology for integrating data sources providing time series data based on signal descriptions."@en ;
                           <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0/> ;
                           <http://purl.org/dc/terms/title> "Signal Description Ontology"@en ;
                           owl:versionInfo "0.1" ;
                           <https://w3id.org/mod#createdWith> <https://chowlk.linkeddata.es/> .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdf:type owl:AnnotationProperty .


###  https://w3id.org/mod#createdWith
<https://w3id.org/mod#createdWith> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://example.org/sdo/core#signalType
<http://example.org/sdo/core#signalType> rdf:type rdfs:Datatype .


###  http://example.org/sdo/opcua#mode
<http://example.org/sdo/opcua#mode> rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/sdo/core#hasAsset
<http://example.org/sdo/core#hasAsset> rdf:type owl:ObjectProperty ,
                                                owl:TransitiveProperty ;
                                       rdfs:domain <http://example.org/sdo/core#Asset> ;
                                       rdfs:range <http://example.org/sdo/core#Asset> ;
                                       rdfs:comment "Links an Asset to another Asset it contains to." ;
                                       rdfs:label "has asset" .


###  http://example.org/sdo/core#hasCredentials
<http://example.org/sdo/core#hasCredentials> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://example.org/sdo/core#ConnectionInfo> ;
                                             rdfs:range <http://example.org/sdo/core#Credentials> ;
                                             rdfs:comment "Links Credentials to a ConnectionInfo providing it for authentication." ;
                                             rdfs:label "has credentials" .


###  http://example.org/sdo/core#hasDataType
<http://example.org/sdo/core#hasDataType> rdf:type owl:ObjectProperty ;
                                          owl:inverseOf <http://example.org/sdo/core#isDataTypeOf> ;
                                          rdfs:domain <http://example.org/sdo/core#SignalDescription> ;
                                          rdfs:range rdfs:Datatype ;
                                          rdfs:comment "Links a Datatype to a SignalDescription it refers to." ;
                                          rdfs:label "has data type" .


###  http://example.org/sdo/core#hasHistAccess
<http://example.org/sdo/core#hasHistAccess> rdf:type owl:ObjectProperty ;
                                            owl:inverseOf <http://example.org/sdo/core#isHistAccessOf> ;
                                            rdfs:domain <http://example.org/sdo/core#SignalDescription> ;
                                            rdfs:range <http://example.org/sdo/core#DataAccess> ;
                                            rdfs:comment "Links historical DataAccess to a SignalDescription providing it." ;
                                            rdfs:label "has historical access" .


###  http://example.org/sdo/core#hasMetadata
<http://example.org/sdo/core#hasMetadata> rdf:type owl:ObjectProperty ;
                                          rdfs:domain <http://example.org/sdo/core#SignalDescription> ;
                                          rdfs:range <http://example.org/sdo/core#MetaData> ;
                                          rdfs:comment "Links a metadata to a SignalDescription described by" ;
                                          rdfs:label "has metadata" .


###  http://example.org/sdo/core#hasNrtAccess
<http://example.org/sdo/core#hasNrtAccess> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <http://example.org/sdo/core#SignalDescription> ;
                                           rdfs:range <http://example.org/sdo/core#DataAccess> ;
                                           rdfs:comment "Links near real-time DataAccess to a SignalDescription providing it." ;
                                           rdfs:label "has near real-time access" .


###  http://example.org/sdo/core#hasSignalDescription
<http://example.org/sdo/core#hasSignalDescription> rdf:type owl:ObjectProperty ;
                                                   owl:inverseOf <http://example.org/sdo/core#isSignalDescriptionOf> ;
                                                   rdfs:domain <http://example.org/sdo/core#Asset> ;
                                                   rdfs:range <http://example.org/sdo/core#SignalDescription> ;
                                                   rdfs:comment "Links a SignalDescription to a Asset it contains to." ;
                                                   rdfs:label "has signal description" .


###  http://example.org/sdo/core#hasUnit
<http://example.org/sdo/core#hasUnit> rdf:type owl:ObjectProperty ;
                                      owl:inverseOf <http://example.org/sdo/core#isUnitOf> ;
                                      rdfs:domain <http://example.org/sdo/core#SignalDescription> ;
                                      rdfs:range <http://qudt.org/schema/qudt/Unit> ;
                                      rdfs:comment "Links a Unit to a SignalDescription its values are measured by." ;
                                      rdfs:label "has unit" .


###  http://example.org/sdo/core#isAssetOf
<http://example.org/sdo/core#isAssetOf> rdf:type owl:ObjectProperty ;
                                        owl:inverseOf <http://example.org/sdo/core#hasAsset> ;
                                        rdf:type owl:TransitiveProperty ;
                                        rdfs:domain <http://example.org/sdo/core#Asset> ;
                                        rdfs:range <http://example.org/sdo/core#Asset> ;
                                        rdfs:comment "Links an Asset to another Asset contained by it." ;
                                        rdfs:label "is asset of" .


###  http://example.org/sdo/core#isCredentialsOf
<http://example.org/sdo/core#isCredentialsOf> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf <http://example.org/sdo/core#hasCredentials> ;
                                              rdfs:domain <http://example.org/sdo/core#Credentials> ;
                                              rdfs:range <http://example.org/sdo/core#ConnectionInfo> ;
                                              rdfs:comment "Links a ConnectionInfo to Credentials provided by it for authentication." ;
                                              rdfs:label "is credentials of" .


###  http://example.org/sdo/core#isDataTypeOf
<http://example.org/sdo/core#isDataTypeOf> rdf:type owl:ObjectProperty ;
                                           rdfs:domain rdfs:Datatype ;
                                           rdfs:range <http://example.org/sdo/core#SignalDescription> ;
                                           rdfs:comment "Links a SignalDescription to a Datatype referred by it." ;
                                           rdfs:label "is data type of" .


###  http://example.org/sdo/core#isHistAccessOf
<http://example.org/sdo/core#isHistAccessOf> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://example.org/sdo/core#DataAccess> ;
                                             rdfs:range <http://example.org/sdo/core#SignalDescription> ;
                                             rdfs:comment "Links SignalDescription to historical DataAccess provided by it." ;
                                             rdfs:label "is historical access of" .


###  http://example.org/sdo/core#isMetadataOf
<http://example.org/sdo/core#isMetadataOf> rdf:type owl:ObjectProperty ;
                                           owl:inverseOf <http://example.org/sdo/core#hasMetadata> ;
                                           rdfs:domain <http://example.org/sdo/core#MetaData> ;
                                           rdfs:range <http://example.org/sdo/core#SignalDescription> ;
                                           rdfs:comment "Links a SignalDescription to a metadata describing it" ;
                                           rdfs:label "is metadata of" .


###  http://example.org/sdo/core#isNrtAccessOf
<http://example.org/sdo/core#isNrtAccessOf> rdf:type owl:ObjectProperty ;
                                            owl:inverseOf <http://example.org/sdo/core#hasNrtAccess> ;
                                            rdfs:domain <http://example.org/sdo/core#DataAccess> ;
                                            rdfs:range <http://example.org/sdo/core#SignalDescription> ;
                                            rdfs:comment "Links SignalDescription to near real-time DataAccess provided by it." ;
                                            rdfs:label "is near real-time access of" .


###  http://example.org/sdo/core#isSignalDescriptionOf
<http://example.org/sdo/core#isSignalDescriptionOf> rdf:type owl:ObjectProperty ;
                                                    rdfs:domain <http://example.org/sdo/core#SignalDescription> ;
                                                    rdfs:range <http://example.org/sdo/core#Asset> ;
                                                    rdfs:comment "Links an Asset to a SignalDescription contained by it." ;
                                                    rdfs:label "is signal description of" .


###  http://example.org/sdo/core#isUnitOf
<http://example.org/sdo/core#isUnitOf> rdf:type owl:ObjectProperty ;
                                       rdfs:domain <http://qudt.org/schema/qudt/Unit> ;
                                       rdfs:range <http://example.org/sdo/core#SignalDescription> ;
                                       rdfs:comment "Links a SignalDescription to a Unit measuring it." ;
                                       rdfs:label "is unit of" .


###  http://example.org/sdo/influxdb#hasInfluxDbConnectionInfo
<http://example.org/sdo/influxdb#hasInfluxDbConnectionInfo> rdf:type owl:ObjectProperty ;
                                                            owl:inverseOf <http://example.org/sdo/influxdb#isInfluxDbConnectionInfoOf> ;
                                                            rdfs:domain <http://example.org/sdo/influxdb#InfluxDbAccess> ;
                                                            rdfs:range <http://example.org/sdo/influxdb#InfluxDbConnectionInfo> ;
                                                            rdfs:comment "Links connection information to InfluxDB Access requiring it." ;
                                                            rdfs:label "has InfluxDB connection information" .


###  http://example.org/sdo/influxdb#hasTag
<http://example.org/sdo/influxdb#hasTag> rdf:type owl:ObjectProperty ;
                                         rdfs:domain <http://example.org/sdo/influxdb#InfluxDbAccess> ;
                                         rdfs:range <http://example.org/sdo/influxdb#Tag> ;
                                         rdfs:comment "Links a Tag identifying a point to InfluxDB Access." ;
                                         rdfs:label "has tag" .


###  http://example.org/sdo/influxdb#isInfluxDbConnectionInfoOf
<http://example.org/sdo/influxdb#isInfluxDbConnectionInfoOf> rdf:type owl:ObjectProperty ;
                                                             rdfs:domain <http://example.org/sdo/influxdb#InfluxDbConnectionInfo> ;
                                                             rdfs:range <http://example.org/sdo/influxdb#InfluxDbAccess> ;
                                                             rdfs:comment "Links InfluxDB Access to connection information required by it." ;
                                                             rdfs:label "is InfluxDB connection information of" .


###  http://example.org/sdo/influxdb#isTagOf
<http://example.org/sdo/influxdb#isTagOf> rdf:type owl:ObjectProperty ;
                                          owl:inverseOf <http://example.org/sdo/influxdb#hasTag> ;
                                          rdfs:domain <http://example.org/sdo/influxdb#Tag> ;
                                          rdfs:range <http://example.org/sdo/influxdb#InfluxDbAccess> ;
                                          rdfs:comment "Links InfluxDB Access to a Tag identifying a point." ;
                                          rdfs:label "is tag of" .


###  http://example.org/sdo/opcua#hasMessageSecurity
<http://example.org/sdo/opcua#hasMessageSecurity> rdf:type owl:ObjectProperty ;
                                                  owl:inverseOf <http://example.org/sdo/opcua#isMessageSecurityOf> ;
                                                  rdfs:domain <http://example.org/sdo/opcua#OpcUaConnectionInfo> ;
                                                  rdfs:range <http://example.org/sdo/opcua#MessageSecurity> ;
                                                  rdfs:comment "Links MessageSecurity to OpcUaConnectionInfo requiring it for secure communication." ;
                                                  rdfs:label "has message security" .


###  http://example.org/sdo/opcua#hasOpcUaConnectionInfo
<http://example.org/sdo/opcua#hasOpcUaConnectionInfo> rdf:type owl:ObjectProperty ;
                                                      rdfs:domain <http://example.org/sdo/opcua#OpcUaAccess> ;
                                                      rdfs:range <http://example.org/sdo/opcua#OpcUaConnectionInfo> ;
                                                      rdfs:comment "Links connection information to OPC UA Access requiring it." ;
                                                      rdfs:label "has OPC UA connection information" .


###  http://example.org/sdo/opcua#isMessageSecurityOf
<http://example.org/sdo/opcua#isMessageSecurityOf> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain <http://example.org/sdo/opcua#MessageSecurity> ;
                                                   rdfs:range <http://example.org/sdo/opcua#OpcUaConnectionInfo> ;
                                                   rdfs:comment "Links OpcUaConnectionInfo to MessageSecurity required by it for secure communication." ;
                                                   rdfs:label "is message security of" .


###  http://example.org/sdo/opcua#isOpcUaConnectionInfoOf
<http://example.org/sdo/opcua#isOpcUaConnectionInfoOf> rdf:type owl:ObjectProperty ;
                                                       owl:inverseOf <http://example.org/sdo/opcua#hasOpcUaConnectionInfo> ;
                                                       rdfs:domain <http://example.org/sdo/opcua#OpcUaConnectionInfo> ;
                                                       rdfs:range <http://example.org/sdo/opcua#OpcUaAccess> ;
                                                       rdfs:comment "Links OPC UA Access to connection information required by it." ;
                                                       rdfs:label "is OPC UA connection information of" .


#################################################################
#    Data properties
#################################################################

###  http://example.org/sdo/core#hasCert
<http://example.org/sdo/core#hasCert> rdf:type owl:DatatypeProperty ;
                                      rdfs:domain <http://example.org/sdo/core#CertCredentials> ;
                                      rdfs:range xsd:anyURI ;
                                      rdfs:comment "The certificate used for certificate-based authentication." ;
                                      rdfs:label "has certificate" .


###  http://example.org/sdo/core#hasKey
<http://example.org/sdo/core#hasKey> rdf:type owl:DatatypeProperty ;
                                     rdfs:domain <http://example.org/sdo/core#KeyValuePair> ;
                                     rdfs:range xsd:string ;
                                     rdfs:comment "The key of a key-value pair." ;
                                     rdfs:label "has key" .


###  http://example.org/sdo/core#hasPassword
<http://example.org/sdo/core#hasPassword> rdf:type owl:DatatypeProperty ;
                                          rdfs:domain <http://example.org/sdo/core#UserPassCredentials> ;
                                          rdfs:range xsd:string ;
                                          rdfs:comment "The password used for username-password credentials." ;
                                          rdfs:label "has password" .


###  http://example.org/sdo/core#hasPrivateKey
<http://example.org/sdo/core#hasPrivateKey> rdf:type owl:DatatypeProperty ;
                                            rdfs:domain <http://example.org/sdo/core#CertCredentials> ;
                                            rdfs:range xsd:anyURI ;
                                            rdfs:comment "The private key used for certificate-based authentication." ;
                                            rdfs:label "has private key" .


###  http://example.org/sdo/core#hasSignalType
<http://example.org/sdo/core#hasSignalType> rdf:type owl:DatatypeProperty ;
                                            rdfs:domain <http://example.org/sdo/core#SignalDescription> ;
                                            rdfs:range [ rdf:type rdfs:Datatype ;
                                                         owl:oneOf [ rdf:type rdf:List ;
                                                                     rdf:first "HoldObservations"^^<http://example.org/sdo/core#signalType> ;
                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                rdf:first "LinearInterpolation"^^<http://example.org/sdo/core#signalType> ;
                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                           rdf:first "Observations"^^<http://example.org/sdo/core#signalType> ;
                                                                                           rdf:rest rdf:nil
                                                                                         ]
                                                                              ]
                                                                   ]
                                                       ] ;
                                            rdfs:comment "A enumeration that determines how values between the data samples are interpreted." ;
                                            rdfs:label "has signal type" .


###  http://example.org/sdo/core#hasToken
<http://example.org/sdo/core#hasToken> rdf:type owl:DatatypeProperty ;
                                       rdfs:domain <http://example.org/sdo/core#TokenCredentials> ;
                                       rdfs:range xsd:string ;
                                       rdfs:comment "The token used for token-based authentication." ;
                                       rdfs:label "has token" .


###  http://example.org/sdo/core#hasURL
<http://example.org/sdo/core#hasURL> rdf:type owl:DatatypeProperty ;
                                     rdfs:domain <http://example.org/sdo/core#ConnectionInfo> ;
                                     rdfs:range xsd:anyURI ;
                                     rdfs:comment "The URL of a connection." ;
                                     rdfs:label "has URL" .


###  http://example.org/sdo/core#hasUsername
<http://example.org/sdo/core#hasUsername> rdf:type owl:DatatypeProperty ;
                                          rdfs:domain <http://example.org/sdo/core#UserPassCredentials> ;
                                          rdfs:range xsd:string ;
                                          rdfs:comment "The username used for username-password credentials." ;
                                          rdfs:label "has username" .


###  http://example.org/sdo/core#hasValue
<http://example.org/sdo/core#hasValue> rdf:type owl:DatatypeProperty ;
                                       rdfs:domain <http://example.org/sdo/core#KeyValuePair> ;
                                       rdfs:range rdfs:Literal ;
                                       rdfs:comment "The value of a key-value pair." ;
                                       rdfs:label "has value" .


###  http://example.org/sdo/influxdb#hasBucket
<http://example.org/sdo/influxdb#hasBucket> rdf:type owl:DatatypeProperty ;
                                            rdfs:domain <http://example.org/sdo/influxdb#InfluxDbConnectionInfo> ;
                                            rdfs:range xsd:string ;
                                            rdfs:comment "The bucket name in InfluxDB." ;
                                            rdfs:label "has bucket" .


###  http://example.org/sdo/influxdb#hasField
<http://example.org/sdo/influxdb#hasField> rdf:type owl:DatatypeProperty ;
                                           rdfs:domain <http://example.org/sdo/influxdb#InfluxDbAccess> ;
                                           rdfs:range xsd:string ;
                                           rdfs:comment "The field key in InfluxDB." ;
                                           rdfs:label "has field" .


###  http://example.org/sdo/influxdb#hasMeasurement
<http://example.org/sdo/influxdb#hasMeasurement> rdf:type owl:DatatypeProperty ;
                                                 rdfs:domain <http://example.org/sdo/influxdb#InfluxDbAccess> ;
                                                 rdfs:range xsd:string ;
                                                 rdfs:comment "The measurement name in InfluxDB." ;
                                                 rdfs:label "has measurement" .


###  http://example.org/sdo/influxdb#hasOrganization
<http://example.org/sdo/influxdb#hasOrganization> rdf:type owl:DatatypeProperty ;
                                                  rdfs:domain <http://example.org/sdo/influxdb#InfluxDbConnectionInfo> ;
                                                  rdfs:range xsd:string ;
                                                  rdfs:comment "The organization name in InfluxDB." ;
                                                  rdfs:label "has organization" .


###  http://example.org/sdo/opcua#hasClientCert
<http://example.org/sdo/opcua#hasClientCert> rdf:type owl:DatatypeProperty ;
                                             rdfs:domain <http://example.org/sdo/opcua#MessageSecurity> ;
                                             rdfs:range xsd:anyURI ;
                                             rdfs:comment "The client certificate used to establish an OPC UA secure channel." ;
                                             rdfs:label "has client certificate" .


###  http://example.org/sdo/opcua#hasClientPrivateKey
<http://example.org/sdo/opcua#hasClientPrivateKey> rdf:type owl:DatatypeProperty ;
                                                   rdfs:domain <http://example.org/sdo/opcua#MessageSecurity> ;
                                                   rdfs:range xsd:anyURI ;
                                                   rdfs:comment "The client's private key used to establish an OPC UA secure channel." ;
                                                   rdfs:label "has client private key" .


###  http://example.org/sdo/opcua#hasIdentifier
<http://example.org/sdo/opcua#hasIdentifier> rdf:type owl:DatatypeProperty ;
                                             rdfs:domain <http://example.org/sdo/opcua#OpcUaAccess> ;
                                             rdfs:range rdfs:Literal ;
                                             rdfs:comment "The identifier used in node IDs." ;
                                             rdfs:label "has identifier" .


###  http://example.org/sdo/opcua#hasMode
<http://example.org/sdo/opcua#hasMode> rdf:type owl:DatatypeProperty ;
                                       rdfs:domain <http://example.org/sdo/opcua#MessageSecurity> ;
                                       rdfs:range [ rdf:type rdfs:Datatype ;
                                                    owl:oneOf [ rdf:type rdf:List ;
                                                                rdf:first "Sign"^^<http://example.org/sdo/opcua#mode> ;
                                                                rdf:rest [ rdf:type rdf:List ;
                                                                           rdf:first "SignAndEncrypt"^^<http://example.org/sdo/opcua#mode> ;
                                                                           rdf:rest rdf:nil
                                                                         ]
                                                              ]
                                                  ] ;
                                       rdfs:comment "A enumeration that defines the message security mode." ;
                                       rdfs:label "has mode" .


###  http://example.org/sdo/opcua#hasNamespaceIndex
<http://example.org/sdo/opcua#hasNamespaceIndex> rdf:type owl:DatatypeProperty ;
                                                 rdfs:domain <http://example.org/sdo/opcua#OpcUaAccess> ;
                                                 rdfs:range xsd:integer ;
                                                 rdfs:comment "The namespace index used in node IDs." ;
                                                 rdfs:label "has namespace index" .


###  http://example.org/sdo/opcua#hasNamespaceURI
<http://example.org/sdo/opcua#hasNamespaceURI> rdf:type owl:DatatypeProperty ;
                                               rdfs:domain <http://example.org/sdo/opcua#OpcUaAccess> ;
                                               rdfs:range xsd:anyURI ;
                                               rdfs:comment "The URI of the namespace used in expanded node IDs." ;
                                               rdfs:label "has namespace URI" .


###  http://example.org/sdo/opcua#hasSecurityPolicy
<http://example.org/sdo/opcua#hasSecurityPolicy> rdf:type owl:DatatypeProperty ;
                                                 rdfs:domain <http://example.org/sdo/opcua#MessageSecurity> ;
                                                 rdfs:range xsd:string ;
                                                 rdfs:comment "The name of the security policy profile defining the cryptographic algorithms for OPC UA message security." ;
                                                 rdfs:label "has security policy" .


###  http://example.org/sdo/opcua#hasServerCert
<http://example.org/sdo/opcua#hasServerCert> rdf:type owl:DatatypeProperty ;
                                             rdfs:domain <http://example.org/sdo/opcua#MessageSecurity> ;
                                             rdfs:range xsd:anyURI ;
                                             rdfs:comment "The server certificate used to establish an OPC UA secure channel." ;
                                             rdfs:label "has server certificate" .


###  http://example.org/sdo/opcua#hasServerIndex
<http://example.org/sdo/opcua#hasServerIndex> rdf:type owl:DatatypeProperty ;
                                              rdfs:domain <http://example.org/sdo/opcua#OpcUaAccess> ;
                                              rdfs:range xsd:integer ;
                                              rdfs:comment "The server index used in expanded node IDs." ;
                                              rdfs:label "has server index" .


#################################################################
#    Classes
#################################################################

###  http://example.org/sdo/core#Asset
<http://example.org/sdo/core#Asset> rdf:type owl:Class ;
                                    rdfs:comment "Represents a physical or logical object that provides signals or contains other assets." ;
                                    rdfs:label "Asset" .


###  http://example.org/sdo/core#CertCredentials
<http://example.org/sdo/core#CertCredentials> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://example.org/sdo/core#Credentials> ;
                                              rdfs:comment "Represents authentication using certificates." ;
                                              rdfs:label "Cert Credentials" .


###  http://example.org/sdo/core#ConnectionInfo
<http://example.org/sdo/core#ConnectionInfo> rdf:type owl:Class ;
                                             owl:disjointUnionOf ( <http://example.org/sdo/influxdb#InfluxDbConnectionInfo>
                                                                   <http://example.org/sdo/opcua#OpcUaConnectionInfo>
                                                                 ) ;
                                             rdfs:comment "Contains the information required to connect to a source." ;
                                             rdfs:label "Connection Info" .


###  http://example.org/sdo/core#Credentials
<http://example.org/sdo/core#Credentials> rdf:type owl:Class ;
                                          owl:disjointUnionOf ( <http://example.org/sdo/core#CertCredentials>
                                                                <http://example.org/sdo/core#TokenCredentials>
                                                                <http://example.org/sdo/core#UserPassCredentials>
                                                              ) ;
                                          rdfs:comment "Serves as supertype for different authentication methods." ;
                                          rdfs:label "Credentials" .


###  http://example.org/sdo/core#DataAccess
<http://example.org/sdo/core#DataAccess> rdf:type owl:Class ;
                                         owl:disjointUnionOf ( <http://example.org/sdo/influxdb#InfluxDbAccess>
                                                               <http://example.org/sdo/opcua#OpcUaAccess>
                                                             ) ;
