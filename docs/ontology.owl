        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
        <rdfs:comment>Links a Datatype to a SignalDescription it refers to.</rdfs:comment>
        <rdfs:label>has data type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/sdo/core#hasHistAccess -->


    <owl:ObjectProperty rdf:about="http://example.org/sdo/core#hasHistAccess">
        <owl:inverseOf rdf:resource="http://example.org/sdo/core#isHistAccessOf"/>
