        <rdfs:range rdf:resource="http://example.org/sdo/core#SignalDescription"/>
        <rdfs:comment>Links SignalDescription to historical DataAccess provided by it.</rdfs:comment>
        <rdfs:label>is historical access of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/sdo/core#isMetadataOf -->


    <owl:ObjectProperty rdf:about="http://example.org/sdo/core#isMetadataOf">
        <owl:inverseOf rdf:resource="http://example.org/sdo/core#hasMetadata"/>
