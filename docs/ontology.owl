    <!-- http://example.org/sdo/core#hasAsset -->


    <owl:ObjectProperty rdf:about="http://example.org/sdo/core#hasAsset">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://example.org/sdo/core#Asset"/>
        <rdfs:range rdf:resource="http://example.org/sdo/core#Asset"/>
        <rdfs:comment>Links an Asset to another Asset it contains to.</rdfs:comment>
        <rdfs:label>has asset</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/sdo/core#hasCredentials -->


    <owl:ObjectProperty rdf:about="http://example.org/sdo/core#hasCredentials">
        <rdfs:domain rdf:resource="http://example.org/sdo/core#ConnectionInfo"/>
        <rdfs:range rdf:resource="http://example.org/sdo/core#Credentials"/>
        <rdfs:comment>Links Credentials to a ConnectionInfo providing it for authentication.</rdfs:comment>
        <rdfs:label>has credentials</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/sdo/core#hasDataType -->


    <owl:ObjectProperty rdf:about="http://example.org/sdo/core#hasDataType">
        <owl:inverseOf rdf:resource="http://example.org/sdo/core#isDataTypeOf"/>
