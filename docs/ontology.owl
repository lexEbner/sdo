        <rdfs:range rdf:resource="http://example.org/sdo/core#DataAccess"/>
        <rdfs:comment>Links historical DataAccess to a SignalDescription providing it.</rdfs:comment>
        <rdfs:label>has historical access</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/sdo/core#hasMetadata -->


    <owl:ObjectProperty rdf:about="http://example.org/sdo/core#hasMetadata">
        <rdfs:domain rdf:resource="http://example.org/sdo/core#SignalDescription"/>
        <rdfs:range rdf:resource="http://example.org/sdo/core#MetaData"/>
        <rdfs:comment>Links a metadata to a SignalDescription described by</rdfs:comment>
        <rdfs:label>has metadata</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/sdo/core#hasNrtAccess -->


    <owl:ObjectProperty rdf:about="http://example.org/sdo/core#hasNrtAccess">
        <rdfs:domain rdf:resource="http://example.org/sdo/core#SignalDescription"/>
        <rdfs:range rdf:resource="http://example.org/sdo/core#DataAccess"/>
        <rdfs:comment>Links near real-time DataAccess to a SignalDescription providing it.</rdfs:comment>
        <rdfs:label>has near real-time access</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/sdo/core#hasSignalDescription -->


    <owl:ObjectProperty rdf:about="http://example.org/sdo/core#hasSignalDescription">
        <owl:inverseOf rdf:resource="http://example.org/sdo/core#isSignalDescriptionOf"/>
        <rdfs:domain rdf:resource="http://example.org/sdo/core#Asset"/>
        <rdfs:range rdf:resource="http://example.org/sdo/core#SignalDescription"/>
        <rdfs:comment>Links a SignalDescription to a Asset it contains to.</rdfs:comment>
        <rdfs:label>has signal description</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/sdo/core#hasUnit -->


    <owl:ObjectProperty rdf:about="http://example.org/sdo/core#hasUnit">
        <owl:inverseOf rdf:resource="http://example.org/sdo/core#isUnitOf"/>
