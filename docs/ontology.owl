    <!-- http://example.org/sdo/core#hasSignalType -->


    <owl:DatatypeProperty rdf:about="http://example.org/sdo/core#hasSignalType">
        <rdfs:domain rdf:resource="http://example.org/sdo/core#SignalDescription"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first rdf:datatype="http://example.org/sdo/core#signalType">HoldObservations</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:datatype="http://example.org/sdo/core#signalType">LinearInterpolation</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://example.org/sdo/core#signalType">Observations</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>A enumeration that determines how values between the data samples are interpreted.</rdfs:comment>
        <rdfs:label>has signal type</rdfs:label>
    </owl:DatatypeProperty>
    


