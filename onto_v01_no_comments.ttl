@prefix : <http://example.org/sdo#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix mod: <https://w3id.org/mod#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix sdo: <http://example.org/sdo#> .
@prefix core: <http://example.org/sdo/core#> .
@prefix opcua: <http://example.org/sdo/opcua#> .
@prefix influxdb: <http://example.org/sdo/influxdb#> .
@base <http://example.org/sdo#> .

<http://example.org/sdo#> a owl:Ontology ;
    dcterms:created "2025-09-25"^^xsd:date ;
    dcterms:creator "http://example.org/people#AlexanderEbner" ;
    dcterms:description "An ontology for integrating data sources providing time series data based on signal descriptions."@en ;
    dcterms:title "Signal Description Ontology"@en ;
    owl:versionInfo "0.1" ;
    mod:createdWith <https://chowlk.linkeddata.es/> ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> .

qudt:Unit rdfs:label "Unit" .

rdfs:Datatype rdfs:label "Datatype" .

# ---------------------------------------------------
# Annotation Properties
# ---------------------------------------------------

dcterms:created a owl:AnnotationProperty .

dcterms:creator a owl:AnnotationProperty .

dcterms:description a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

dcterms:license rdf:type owl:AnnotationProperty .

owl:versionInfo a owl:AnnotationProperty .

mod:createdWith rdf:type owl:AnnotationProperty .

# ---------------------------------------------------
# Data Types
# ---------------------------------------------------

core:signalType rdf:type rdfs:Datatype .

opcua:mode rdf:type rdfs:Datatype .

# ---------------------------------------------------
# Core Classes
# ---------------------------------------------------

core:Asset a owl:Class ;
    rdfs:label "Asset" ;
    rdfs:comment "The Asset." .

core:SignalDescription a owl:Class ;
    rdfs:label "Signal Description" .

core:ConnectionInfo a owl:Class ;
    rdfs:label "Connection Info" ;
    owl:disjointUnionOf (influxdb:InfluxDbConnectionInfo opcua:OpcUaConnectionInfo) .

core:Credentials a owl:Class ;
    rdfs:label "Credentials" ;
    owl:disjointUnionOf (core:UserPassCredentials core:CertCredentials core:TokenCredentials) .

core:DataAccess a owl:Class ;
    rdfs:label "Data Access" ;
    owl:disjointUnionOf (influxdb:InfluxDbAccess opcua:OpcUaAccess) .

core:KeyValuePair a owl:Class ;
    rdfs:label "Key-Value Pair" ;
    owl:disjointUnionOf (core:MetaData influxdb:Tag) .

core:MetaData a owl:Class ;
    rdfs:label "Meta Data" ;
    rdfs:subClassOf core:KeyValuePair .

core:TokenCredentials a owl:Class ;
    rdfs:label "Token Credentials" ;
    rdfs:subClassOf core:Credentials .

core:CertCredentials a owl:Class ;
    rdfs:label "Cert Credentials" ;
    rdfs:subClassOf core:Credentials .

core:UserPassCredentials a owl:Class ;
    rdfs:label "User Pass Credentials" ;
    rdfs:subClassOf core:Credentials .

# ---------------------------------------------------
# Core Properties
# ---------------------------------------------------

core:hasAsset a owl:ObjectProperty , owl:TransitiveProperty ;
    rdfs:label "has asset" ;
    rdfs:domain core:Asset ;
    rdfs:range core:Asset ;
    owl:inverseOf core:isAssetOf .

core:isAssetOf a owl:ObjectProperty , owl:TransitiveProperty ;
    rdfs:label "is asset of" ;
    rdfs:domain core:Asset ;
    rdfs:range core:Asset ;
    owl:inverseOf core:hasAsset .

core:hasSignalDescription a owl:ObjectProperty ;
    rdfs:label "has signal description" ;
    rdfs:domain core:Asset ;
    rdfs:range core:SignalDescription ;
    owl:inverseOf core:isSignalDescriptionOf .

core:isSignalDescriptionOf a owl:ObjectProperty ;
    rdfs:label "is signal description of" ;
    rdfs:domain core:SignalDescription ;
    rdfs:range core:Asset ;
    owl:inverseOf core:hasSignalDescription .

core:hasSignalType a owl:DatatypeProperty ;
    rdfs:label "has signal type" ;
    rdfs:domain core:SignalDescription ;
    rdfs:range [ a rdfs:Datatype ;
            owl:oneOf [ a rdf:List ;
                    rdf:first "Observations"^^core:signalType ;
                    rdf:rest [ a rdf:List ;
                            rdf:first "HoldObservations"^^core:signalType ;
                            rdf:rest [ a rdf:List ;
                                    rdf:first "LinearInterpolation"^^core:signalType ;
                                    rdf:rest () ] ] ] ] .

core:hasDataType a owl:ObjectProperty ;
    rdfs:label "has data type" ;
    rdfs:domain core:SignalDescription ;
    rdfs:range rdfs:Datatype ;
    owl:inverseOf core:isDataTypeOf .

core:isDataTypeOf a owl:ObjectProperty ;
    rdfs:label "is data type of" ;
    rdfs:domain rdfs:Datatype ;
    rdfs:range core:SignalDescription ;
    owl:inverseOf core:hasDataType .

core:hasUnit a owl:ObjectProperty ;
    rdfs:label "has unit" ;
    rdfs:domain core:SignalDescription ;
    rdfs:range qudt:Unit ;
    owl:inverseOf core:isUnitOf .

core:isUnitOf a owl:ObjectProperty ;
    rdfs:label "is unit of" ;
    rdfs:domain qudt:Unit ;
    rdfs:range core:SignalDescription ;
    owl:inverseOf core:hasUnit .

core:hasHistAccess a owl:ObjectProperty ;
    rdfs:label "has historical access" ;
    rdfs:domain core:SignalDescription ;
    rdfs:range core:DataAccess ;
    owl:inverseOf core:isHistAccessOf .

core:isHistAccessOf a owl:ObjectProperty ;
    rdfs:label "is historical access of" ;
    rdfs:domain core:DataAccess ;
    rdfs:range core:SignalDescription ;
    owl:inverseOf core:hasHistAccess .

core:hasNrtAccess a owl:ObjectProperty ;
    rdfs:label "has near real-time access" ;
    rdfs:domain core:SignalDescription ;
    rdfs:range core:DataAccess ;
    owl:inverseOf core:isNrtAccessOf .

core:isNrtAccessOf a owl:ObjectProperty ;
    rdfs:label "is near real-time access of" ;
    rdfs:domain core:DataAccess ;
    rdfs:range core:SignalDescription ;
    owl:inverseOf core:hasNrtAccess .

core:hasKey a owl:DatatypeProperty ;
    rdfs:label "has key" ;
    rdfs:domain core:KeyValuePair ;
    rdfs:range xsd:string .

core:hasValue a owl:DatatypeProperty ;
    rdfs:label "has value" ;
    rdfs:domain core:KeyValuePair ;
    rdfs:range rdfs:Literal .

core:hasMetadata a owl:ObjectProperty ;
    rdfs:label "has metadata" ;
    rdfs:domain core:SignalDescription ;
    rdfs:range core:MetaData ;
    owl:inverseOf core:isMetadataOf .

core:isMetadataOf a owl:ObjectProperty ;
    rdfs:label "is metadata of" ;
    rdfs:domain core:MetaData ;
    rdfs:range core:SignalDescription ;
    owl:inverseOf core:hasMetadata .

core:hasURL a owl:DatatypeProperty ;
    rdfs:label "has URL" ;
    rdfs:domain core:ConnectionInfo ;
    rdfs:range xsd:anyURI .

core:hasCredentials a owl:ObjectProperty ;
    rdfs:label "has credentials" ;
    rdfs:domain core:ConnectionInfo ;
    rdfs:range core:Credentials ;
    owl:inverseOf core:isCredentialsOf .

core:isCredentialsOf a owl:ObjectProperty ;
    rdfs:label "is credentials of" ;
    rdfs:domain core:Credentials ;
    rdfs:range core:ConnectionInfo ;
    owl:inverseOf core:hasCredentials .

core:hasUsername a owl:DatatypeProperty ;
    rdfs:label "has username" ;
    rdfs:domain core:UserPassCredentials ;
    rdfs:range xsd:string .

core:hasPassword a owl:DatatypeProperty ;
    rdfs:label "has password" ;
    rdfs:domain core:UserPassCredentials ;
    rdfs:range xsd:string .

core:hasCert a owl:DatatypeProperty ;
    rdfs:label "has certificate" ;
    rdfs:domain core:CertCredentials ;
    rdfs:range xsd:anyURI .

core:hasPrivateKey a owl:DatatypeProperty ;
    rdfs:label "has private key" ;
    rdfs:domain core:CertCredentials ;
    rdfs:range xsd:anyURI .

core:hasToken a owl:DatatypeProperty ;
    rdfs:label "has token" ;
    rdfs:domain core:TokenCredentials ;
    rdfs:range xsd:string .

# ---------------------------------------------------
# OPC UA Classes
# ---------------------------------------------------

opcua:OpcUaConnectionInfo a owl:Class ;
    rdfs:label "Opc Ua Connection Info" ;
    rdfs:subClassOf core:ConnectionInfo .

opcua:OpcUaAccess a owl:Class ;
    rdfs:label "Opc Ua Access" ;
    rdfs:subClassOf core:DataAccess .

opcua:MessageSecurity a owl:Class ;
    rdfs:label "Message Security" .

# ---------------------------------------------------
# OPC UA Properties
# ---------------------------------------------------

opcua:hasServerIndex a owl:DatatypeProperty ;
    rdfs:label "has server index" ;
    rdfs:domain opcua:OpcUaAccess ;
    rdfs:range xsd:integer .

opcua:hasNamespaceURI a owl:DatatypeProperty ;
    rdfs:label "has namespace URI" ;
    rdfs:domain opcua:OpcUaAccess ;
    rdfs:range xsd:anyURI .

opcua:hasNamespaceIndex a owl:DatatypeProperty ;
    rdfs:label "has namespace index" ;
    rdfs:domain opcua:OpcUaAccess ;
    rdfs:range xsd:integer .

opcua:hasIdentifier a owl:DatatypeProperty ;
    rdfs:label "has identifier" ;
    rdfs:domain opcua:OpcUaAccess ;
    rdfs:range rdfs:Literal .

opcua:hasOpcUaConnectionInfo a owl:ObjectProperty ;
    rdfs:label "has OPC UA connection information" ;
    rdfs:domain opcua:OpcUaAccess ;
    rdfs:range opcua:OpcUaConnectionInfo ;
    owl:inverseOf opcua:isOpcUaConnectionInfoOf .

opcua:isOpcUaConnectionInfoOf a owl:ObjectProperty ;
    rdfs:label "is OPC UA connection information of" ;
    rdfs:domain opcua:OpcUaConnectionInfo ;
    rdfs:range opcua:OpcUaAccess ;
    owl:inverseOf opcua:hasOpcUaConnectionInfo .

opcua:hasMessageSecurity a owl:ObjectProperty ;
    rdfs:label "has message security" ;
    rdfs:domain opcua:OpcUaConnectionInfo ;
    rdfs:range opcua:MessageSecurity ;
    owl:inverseOf opcua:isMessageSecurityOf .

opcua:isMessageSecurityOf a owl:ObjectProperty ;
    rdfs:label "is message security of" ;
    rdfs:domain opcua:MessageSecurity ;
    rdfs:range opcua:OpcUaConnectionInfo ;
    owl:inverseOf opcua:hasMessageSecurity .

opcua:hasSecurityPolicy a owl:DatatypeProperty ;
    rdfs:label "has security policy" ;
    rdfs:domain opcua:MessageSecurity ;
    rdfs:range xsd:string .

opcua:hasMode a owl:DatatypeProperty ;
    rdfs:label "has mode" ;
    rdfs:domain opcua:MessageSecurity ;
    rdfs:range [ a rdfs:Datatype ;
            owl:oneOf [ a rdf:List ;
                    rdf:first "Sign"^^opcua:mode ;
                    rdf:rest [ a rdf:List ;
                            rdf:first "SignAndEncrypt"^^opcua:mode ;
                            rdf:rest () ] ] ] .

opcua:hasServerCert a owl:DatatypeProperty ;
    rdfs:label "has server certificate" ;
    rdfs:domain opcua:MessageSecurity ;
    rdfs:range xsd:anyURI .

opcua:hasClientCert a owl:DatatypeProperty ;
    rdfs:label "has client certificate" ;
    rdfs:domain opcua:MessageSecurity ;
    rdfs:range xsd:anyURI .

opcua:hasClientPrivateKey a owl:DatatypeProperty ;
    rdfs:label "has client private key" ;
    rdfs:domain opcua:MessageSecurity ;
    rdfs:range xsd:anyURI .

# ---------------------------------------------------
# InfluxDB Classes
# ---------------------------------------------------

influxdb:InfluxDbAccess a owl:Class ;
    rdfs:label "Influx Db Access" ;
    rdfs:subClassOf core:DataAccess .

influxdb:InfluxDbConnectionInfo a owl:Class ;
    rdfs:label "Influx Db Connection Info" ;
    rdfs:subClassOf core:ConnectionInfo .

influxdb:Tag a owl:Class ;
    rdfs:label "Tag" ;
    rdfs:subClassOf core:KeyValuePair .

# ---------------------------------------------------
# InfluxDB Properties
# ---------------------------------------------------

influxdb:hasMeasurement a owl:DatatypeProperty ;
    rdfs:label "has measurement" ;
    rdfs:domain influxdb:InfluxDbAccess ;
    rdfs:range xsd:string .

influxdb:hasField a owl:DatatypeProperty ;
    rdfs:label "has field" ;
    rdfs:domain influxdb:InfluxDbAccess ;
    rdfs:range xsd:string .

influxdb:hasTag a owl:ObjectProperty ;
    rdfs:label "has tag" ;
    rdfs:domain influxdb:InfluxDbAccess ;
    rdfs:range influxdb:Tag ;
    owl:inverseOf influxdb:isTagOf .

influxdb:isTagOf a owl:ObjectProperty ;
    rdfs:label "is tag of" ;
    rdfs:domain influxdb:Tag ;
    rdfs:range influxdb:InfluxDbAccess ;
    owl:inverseOf influxdb:hasTag .

influxdb:hasInfluxDbConnectionInfo a owl:ObjectProperty ;
    rdfs:label "has InfluxDB connection information" ;
    rdfs:domain influxdb:InfluxDbAccess ;
    rdfs:range influxdb:InfluxDbConnectionInfo ;
    owl:inverseOf influxdb:isInfluxDbConnectionInfoOf .

influxdb:isInfluxDbConnectionInfoOf a owl:ObjectProperty ;
    rdfs:label "is InfluxDB connection information of" ;
    rdfs:domain influxdb:InfluxDbConnectionInfo ;
    rdfs:range influxdb:InfluxDbAccess ;
    owl:inverseOf influxdb:hasInfluxDbConnectionInfo .

influxdb:hasBucket a owl:DatatypeProperty ;
    rdfs:label "has bucket" ;
    rdfs:domain influxdb:InfluxDbConnectionInfo ;
    rdfs:range xsd:string .

influxdb:hasOrganization a owl:DatatypeProperty ;
    rdfs:label "has organization" ;
    rdfs:domain influxdb:InfluxDbConnectionInfo ;
    rdfs:range xsd:string .
